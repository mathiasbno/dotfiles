 [alias]
  co    = checkout
  ci    = commit
  st    = status
  br    = branch
  df    = diff
  hist  = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  last  = "!f() { git hist --max-count $1; }; f"

